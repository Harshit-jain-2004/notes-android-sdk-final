<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <size
                android:width="32dp"
                android:height="32dp" />
            <solid android:color="@color/sn_note_color_grey_medium" />
            <stroke
                android:width="1dp"
                android:color="@color/sn_note_color_grey_dark" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/sn_color_item_selector"
        android:gravity="center"
        android:tint="@color/sn_black" />
</layer-list>

